{
    "manifest_version": 2,

    "name": "SupremeOATC",
    "description": "Supreme Open-Source add to cart bot",
    "version": "0.1",
    "background": {
        "scripts": ["js/lib/jquery-3.2.1.min.js", "js/storage.js", "js/utils.js", "js/background.js"]
    },
    "browser_action": {
        "default_popup": "options.html",
        "default_icon": "icon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "contentSettings",
        "cookies",
        "storage",
        "webNavigation",
        "background",
        "notifications"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.supremenewyork.com/*"
        ],
        "js": ["js/utils.js", "js/storage.js", "js/lib/jquery-3.2.1.min.js", "js/BaseManager.js", "js/supreme.js"]
    }]
}