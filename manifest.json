{
    "manifest_version": 2,

    "name": "Simple bot",
    "description": "A simple bot",
    "version": "0.1",
    "browser_action": {
        "default_popup": "options.html"
    },
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "contentSettings",
        "cookies",
        "storage",
        "webNavigation"
    ],
    "content_scripts": [{
            "matches": [
                "*://*.supremenewyork.com/*"
            ],
            "js": ["js/storage.js", "js/supreme.js"]
        },
        {
            "matches": [
                "*://*.supremenewyork.com/checkout"
            ],
            "js": ["js/storage.js", "js/jquery-3.2.1.min.js", "js/checkout.js"]
        },
        {
            "matches": [
                "*://*.supremenewyork.com/shop/cart"
            ],
            "js": ["js/storage.js", "js/cart.js"]
        }
    ]
}