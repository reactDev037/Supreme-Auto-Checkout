{
    "manifest_version": 2,

    "name": "Simple bot",
    "description": "A simple bot",
    "version": "0.1",
    "browser_action": {
        "default_popup": "options.html",
        "default_icon": "icon.png"
    },
    "options_page": "options.html",
    "permissions": [
        "activeTab",
        "contentSettings",
        "cookies",
        "storage",
        "webNavigation",
        "tabs"
    ],
    "content_scripts": [{
        "matches": [
            "*://*.supremenewyork.com/*"
        ],
        "js": ["js/storage.js", "js/lib/jquery-3.2.1.min.js", "js/supreme.js"]
    }]
}